<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rootOD"
    android:layout_width="match_parent"
    android:layout_height="match_parent">




    <ImageView
        android:id="@+id/imgAirConditionOD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_29"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        />

    <TextView
        android:id="@+id/txtAirConditionOD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/order_air_condition"
        android:textColor="@color/colorBlack2"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/imgAirConditionOD"
        app:layout_constraintTop_toTopOf="@id/imgAirConditionOD"
        app:layout_constraintBottom_toBottomOf="@id/imgAirConditionOD"
        android:layout_marginStart="20dp"
        />

    <ImageView
        android:id="@+id/imgBaggageOD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_30"
        app:layout_constraintTop_toBottomOf="@id/imgAirConditionOD"
        app:layout_constraintStart_toStartOf="@id/imgAirConditionOD"
        android:layout_marginTop="20dp"
         />

    <TextView
        android:id="@+id/txtBaggageOD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/order_baggage"
        android:textColor="@color/colorBlack2"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/imgBaggageOD"
        app:layout_constraintTop_toTopOf="@id/imgBaggageOD"
        app:layout_constraintBottom_toBottomOf="@id/imgBaggageOD"
        android:layout_marginStart="20dp"
         />

    <ImageView
        android:id="@+id/imgTelevisionOD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_28"
        app:layout_constraintTop_toBottomOf="@id/imgBaggageOD"
        app:layout_constraintStart_toStartOf="@id/imgAirConditionOD"
        android:layout_marginTop="20dp"/>


    <TextView
        android:id="@+id/txtTelevisionOD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/order_television"
        android:textColor="@color/colorBlack2"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/imgTelevisionOD"
        app:layout_constraintTop_toTopOf="@id/imgTelevisionOD"
        app:layout_constraintBottom_toBottomOf="@id/imgTelevisionOD"
        android:layout_marginStart="20dp" />


</androidx.constraintlayout.widget.ConstraintLayout>